<?xml version="1.0"?>
<!--
Das von allen Zuschauern zu sehende Kind läuft.
-->
<SATZ kind="Hauptsatz">
  <VORFELD>
    <ARGUMENT role="Läufer" case="Nominativ">
      <PHRASE case="Nominativ">
        <DETERMINATIV>
          <ARTIKEL>das</ARTIKEL>
        </DETERMINATIV>
        <SATZ kind="Partizipsatz" controller="Läufer">
          <ARGUMENT role="Seher" case="Nominativ">
            <PARTIZIPSATZ juncture="von">
              <JUNKTOR>von</JUNKTOR>
              <PHRASE case="Dativ">
                <DETERMINATIV>
                  <ARTIKEL>allen</ARTIKEL>
                </DETERMINATIV>
                <REFERENT>Zuschauern</REFERENT>
              </PHRASE>
            </PARTIZIPSATZ>
          </ARGUMENT>
          <ARGUMENT role="Gesehene" case="Akkusativ">
            <PARTIZIPSATZ case="Nominativ">
              <PHRASE controller="Läufer" />
            </PARTIZIPSATZ>
          </ARGUMENT>
          <PRÄDIKAT verb="sehen">
            <INFINITUM verb="sehen" non-finite="Futurpartizip">zu sehende</INFINITUM>
          </PRÄDIKAT>
        </SATZ>
        <REFERENT>Kind</REFERENT>
      </PHRASE>
    </ARGUMENT>
  </VORFELD>
  <VERBZWEIT verb="laufen" tense="Präsens" person="3" number="Singular">läuft</VERBZWEIT>
  <PRÄDIKAT verb="laufen">
    <FINITUM move="Verbzweit" />
  </PRÄDIKAT>
</SATZ>
